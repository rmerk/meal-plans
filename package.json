{
  "$schema": "./.vscode/schemas/package.schema.json",
  "name": "meal-plans",
  "private": true,
  "type": "module",
  "description": "Weekly meal planning with batch cooking strategies - Nuxt 4 PWA",
  "scripts": {
    "build": "nuxt build",
    "dev": "lsof -ti :4000 | xargs kill -9 2>/dev/null || true && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "oxlint",
    "typecheck": "nuxt typecheck",
    "test:browser:start": "node agent-tools/browser-tools/start.js --headless",
    "test:browser:nav": "node agent-tools/browser-tools/nav.js",
    "test:screenshot": "node agent-tools/browser-tools/screenshot.js",
    "test:colors": "node agent-tools/browser-tools/extract-colors.js",
    "test:compare": "node agent-tools/browser-tools/compare-screenshots.js",
    "test:cleanup": "rm -rf tests/screenshots/current/* tests/screenshots/diffs/*",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report test-results/html"
  },
  "dependencies": {
    "@iconify-json/simple-icons": "^1.2.58",
    "@nuxt/content": "^3.8.2",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxtjs/mdc": "0.18.4",
    "@pinia/nuxt": "^0.11.3",
    "@vite-pwa/nuxt": "^1.0.7",
    "@vueuse/nuxt": "^14.0.0",
    "chart.js": "^4.5.1",
    "nuxt": "^4.2.1",
    "nuxt-charts": "^1.0.2",
    "pinia": "^3.0.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@iconify-json/lucide": "^1.2.73",
    "@nuxt/eslint": "^1.10.0",
    "@playwright/test": "^1.56.1",
    "better-sqlite3": "^12.4.1",
    "eslint": "^9.39.1",
    "kill-port": "^2.0.1",
    "oxlint": "^1.28.0",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vue": "^3.5.23",
    "vue-tsc": "^3.1.3"
  },
  "packageManager": "pnpm@10.21.0",
  "engines": {
    "node": ">=24.2.0",
    "pnpm": ">=10.21.0"
  }
}
